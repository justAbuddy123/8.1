#include <iostream>
using namespace std;
int main() {
    string A = "101101";
    string B = "110011";
    int int_A = stoi(A, nullptr, 2);
    int int_B = stoi(B, nullptr, 2);
    int and_result = int_A & int_B;
    int sum_result = int_A + int_B;
    cout << "Побитовая: " << and_result << endl;
    cout << "Сумма чисел: " << sum_result << endl;
    return 0;
}
